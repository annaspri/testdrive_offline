      //taken from old code, no lazy loading considerations
      function startIntro(){
        var hints;
      
        hints = introJs().addHints();
        console.log('hints added')
        hints.onhintclose(function(e) {
         literacy_counter++;
         if(literacy_counter == 4) {
           $('#clickAllDotsFree2').hide();
           $( ".settings3" ).addClass("green");
         }
        });

          
      };
      
      $('#settings3').on('click', function () { 
        if(literacy_counter != 4){
          $('#clickAllDotsFree2').show();
        }
      });

      $(window).on("load", function() {
        startIntro();
      });
      
    
      
